{
    "name": "indent2to4",
    "displayName": "Indent 2-to-4",
    "description": "Convert indentation from tab or 2 spaces into 4 spaces",
    "version": "0.1.2",
    "publisher": "aaruel",
    "engines": {
        "vscode": "^0.10.1"
    },
    "icon": "icon.png",
    "galleryBanner.color": "#0D5CAB",
    "keywords": [
        "indent",
        "tab",
        "space",
        "spacing"
    ],
    "categories": [
        "Other"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/compulim/vscode-indent-4to2.git"
    },
    "bugs": "https://github.com/compulim/vscode-indent-4to2/issues/",
    "homepage": "https://github.com/compulim/vscode-indent-4to2/blob/master/README.md",
    "activationEvents": [
        "onCommand:2to4.convert2to4"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "2to4.convert2to4",
            "title": "Convert indentation from tab or 2 spaces into 4 spaces"
        }]
    },
    "devDependencies": {
        "vscode": "^0.11.x"
    }
}